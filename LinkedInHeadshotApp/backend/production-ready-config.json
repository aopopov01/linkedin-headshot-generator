{
  "production": {
    "server": {
      "port": 3000,
      "environment": "production",
      "cluster": {
        "enabled": true,
        "workers": "auto"
      },
      "compression": {
        "enabled": true,
        "level": 6,
        "threshold": 1024
      },
      "security": {
        "helmet": true,
        "cors": {
          "enabled": true,
          "origins": ["https://omnishot.app", "https://app.omnishot.com"],
          "credentials": true
        },
        "rateLimit": {
          "windowMs": 900000,
          "max": 1000,
          "skipSuccessfulRequests": false
        }
      }
    },
    "imageProcessing": {
      "sharp": {
        "concurrency": 4,
        "cache": {
          "items": 100,
          "files": 20,
          "memory": 52428800
        },
        "simd": true,
        "limitInputPixels": 268402689
      },
      "optimization": {
        "qualitySettings": {
          "small": {
            "jpeg": 85,
            "webp": 80
          },
          "medium": {
            "jpeg": 90,
            "webp": 85
          },
          "large": {
            "jpeg": 95,
            "webp": 90
          }
        },
        "formats": {
          "useWebP": true,
          "useAVIF": false,
          "progressive": true,
          "optimizeScans": true,
          "mozjpeg": true
        },
        "sharpening": {
          "small": {
            "sigma": 0.5,
            "m1": 1.0,
            "m2": 1.5
          },
          "medium": {
            "sigma": 1.0,
            "m1": 1.2,
            "m2": 2.0
          },
          "large": {
            "sigma": 1.5,
            "m1": 1.5,
            "m2": 2.5
          }
        }
      }
    },
    "concurrency": {
      "maxConcurrentJobs": 8,
      "queueTimeout": 30000,
      "retryAttempts": 3,
      "batchSize": 4,
      "priorityLevels": ["high", "normal", "low"]
    },
    "memory": {
      "maxHeapSizeMB": 512,
      "gcInterval": 30000,
      "bufferPoolSize": 20,
      "memoryThresholdMB": 400,
      "cleanupInterval": 60000
    },
    "monitoring": {
      "enabled": true,
      "metricsInterval": 5000,
      "healthCheckInterval": 30000,
      "performanceTracking": true,
      "errorTracking": true,
      "customMetrics": {
        "imageProcessingTime": true,
        "concurrentUsers": true,
        "memoryUsage": true,
        "platformSuccessRates": true
      }
    },
    "logging": {
      "level": "info",
      "format": "json",
      "rotation": {
        "enabled": true,
        "maxSize": "100MB",
        "maxFiles": 5
      },
      "performance": {
        "logSlowRequests": true,
        "slowRequestThreshold": 1000
      }
    },
    "caching": {
      "redis": {
        "enabled": true,
        "host": "localhost",
        "port": 6379,
        "ttl": 3600,
        "maxMemoryPolicy": "allkeys-lru"
      },
      "platformSpecs": {
        "enabled": true,
        "ttl": 86400
      },
      "imageResults": {
        "enabled": true,
        "ttl": 3600,
        "maxSize": "100MB"
      }
    },
    "platforms": {
