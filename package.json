{
  "name": "linkedin-headshot",
  "version": "1.0.0",
  "description": "A mobile app that generates professional LinkedIn headshots using AI, targeting job seekers, career changers, and professionals who need high-quality headshots quickly and affordably. Built with React Native for cross-platform deployment on iOS and Android app stores.",
  "main": "demo-server.js",
  "scripts": {
    "start": "node demo-server.js",
    "test": "cd scripts && npm run test:all",
    "test:unit": "cd scripts && npm run test:unit",
    "test:integration": "cd scripts && npm run test:integration",
    "test:e2e": "cd scripts && npm run test:e2e",
    "test:security": "cd scripts && npm run test:security",
    "test:performance": "cd scripts && npm run test:performance",
    "test:accessibility": "cd scripts && npm run test:accessibility",
    "test:validation": "cd scripts && npm run test:validation",
    "test:pr": "cd scripts && npm run test:pr",
    "test:main": "cd scripts && npm run test:main",
    "test:nightly": "cd scripts && npm run test:nightly",
    "test:deployment": "cd scripts && npm run test:deployment",
    "test:mobile": "cd LinkedInHeadshotApp && npm test",
    "test:backend": "cd backend && npm test",
    "build:mobile:ios": "cd LinkedInHeadshotApp && npx react-native build-ios",
    "build:mobile:android": "cd LinkedInHeadshotApp && cd android && ./gradlew assembleRelease",
    "build:backend": "cd backend && npm run build",
    "setup:test-env": "cd scripts && npm install && cd ../security-testing && npm install && cd ../performance-testing && npm install && cd ../accessibility-testing && npm install && cd ../app-store-validation && npm install",
    "clean:test-artifacts": "rm -rf reports/* && rm -rf */reports/* && rm -rf */test-results/* && rm -rf */coverage/*",
    "reports:serve": "cd scripts && npm run reports:serve",
    "config:test": "cd scripts && npm run config:show",
    "health:check": "cd scripts && npm run health:check",
    "ci:install-deps": "npm install && cd LinkedInHeadshotApp && npm install && cd ../backend && npm install && cd ../scripts && npm install",
    "ci:quality-check": "cd scripts && node test-runner.js run --trigger pull_request --environment development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aopopov01/linkedin-headshot-generator.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/aopopov01/linkedin-headshot-generator/issues"
  },
  "homepage": "https://github.com/aopopov01/linkedin-headshot-generator#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2"
  }
}
